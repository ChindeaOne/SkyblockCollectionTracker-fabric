{
	"schemaVersion": 1,
	"id": "skyblockcollectiontracker",
	"version": "${version}",
	"name": "SkyblockCollectionTracker",
	"description": "A Minecraft mod that tracks collections in Hypixel Skyblock.",
	"authors": [
		"Chindea_YTB"
	],
	"contact": {
		"sources": "https://github.com/ChindeaOne/SkyblockCollectionTracker-fabric",
		"issues": "https://github.com/ChindeaOne/SkyblockCollectionTracker-fabric/issues"
	},
	"license": "LGPL-2.1",
    "icon": "assets/skyblockcollectiontracker/logo.png",
	"environment": "client",
	"entrypoints": {
		"main": [
			{
				"value": "io.github.chindeaone.collectiontracker.ModLoader",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"mixins.sct.json"
	],
	"depends": {
		"fabricloader": ">=0.18.1",
		"minecraft": "${minecraft_version}",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	}
}