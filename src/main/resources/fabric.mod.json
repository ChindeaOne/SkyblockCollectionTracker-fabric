	"authors": [
		"Chindea_YTB"
	],
	"contact": {
		"sources": "https://github.com/ChindeaOne/SkyblockCollectionTracker"
	},
	"license": "LGPL-2.1",
    "icon": "assets/skyblockcollectiontracker/logo.png",
